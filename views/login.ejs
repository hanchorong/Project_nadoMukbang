  <body>
    <div class="login">
      <div class="loginBox shadow">
        <a class="logo" href="/">nado mukbang</a>
        <div class="inputArea">
          <div>
            <input type="text" id="userId" placeholder="아이디" />
            <span></span>
          </div>
          <div>
            <input type="password" id="userPassword" placeholder="비밀번호" />
            <span></span>
          </div>
          <button id="loginSubmit">Login</button>
          <hr />
          <div class="bottom">
            <a href="#">아이디/비밀번호 찾기</a>
            <a href="#">회원가입</a>
          </div>
        </div>
      </div>
    </div>
