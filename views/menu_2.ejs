          <div class="card col col-md-6 col-12" style="width: 18rem">
            <img
              src="../static/img/foodCategory/wes<%=i +1%>.webp"
              class="card-img-top"
              alt="..."
            />
            <div class="card-body">
              <p class="card-text">
                <%=wesA[i].name%>
              </p>
              <p class="card-text">
                <%=wesA[i].store%>
              </p>
              <p class="card-text">
                <%=wesA[i].menu%>
              </p>
              <p class="card-text">
                <%=wesA[i].tel%>
              </p>
            </div>
          </div>
          <% } %>
        </div>
        <div class="des row"  style="display: none;">
          <%var desA = [
          { name: "시내", store: "편의점디저트", menu: "초콜릿우유아이스크림, 케이크 등", tel: "02-123-2345" }];%>
