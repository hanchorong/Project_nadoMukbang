  <body class="menu_2">
    <%-include("nav.ejs")%>
    <section class="sec1">
      <div class="title"><i class="fa-solid fa-utensils"></i></div>
    </section>
    <section class="sec2">
      <div class="foodCategory">
        <button type="button" id="western">
          <i class="fa-solid fa-pizza-slice"></i><span>양식</span>
        </button>
        <button type="button" id="desert">
          <i class="fa-solid fa-ice-cream"></i><span>디저트</span>
        </button>
        <button type="button" id="chicken">
          <i class="fa-solid fa-drumstick-bite"></i><span>치킨</span>
        </button>
        <button type="button" id="kor">
          <i class="fa-solid fa-bowl-food"></i><span>한식</span>
        </button>
        <button type="button" id="chi">
          <i class="fa-solid fa-bell-concierge"></i><span>중식</span>
        </button>
        <button type="button" id="jap">
          <i class="fa-solid fa-fish-fins"></i><span>일식</span>
        </button>
      </div>
    </section>
    <section class="sec3">
      <div class="cateTitle"></div>
      <hr />
      <div class="ViewArea" >
        <div class="chicken row" style="display: none;">
          <%var chickenA = [
          { name: "문복희", store: "옛날통닭다리", menu: "대왕닭껍질튀김", tel: "02-123-2345" },{ name: "HONG SOUND", store: "홈플러스", menu: "당당치킨", tel: "02-123-2345" },{name: "희밥", store: "교촌, bbq 외 3", menu:"반반콤보, 황금올리브 외 3", tel: "02-123-2345"},{name: "가르마", store: "훌랄라치킨", menu:"참숯핫바베큐매운맛", tel: "02-123-2345"}];%>
          <% for( var i = 0; i<chickenA.length; i++ ) { %>
          <div class="card col col-md-6 col-12" style="width: 18rem">
            <img
              src="../static/img/foodCategory/chicken<%=i +1%>.webp"
              class="card-img-top"
              alt="..."
            />
            <div class="card-body">
              <p class="card-text">
                <%=chickenA[i].name%>
              </p>
              <p class="card-text">
                <%=chickenA[i].store%>
              </p>
              <p class="card-text">
                <%=chickenA[i].menu%>
              </p>
              <p class="card-text">
                <%=chickenA[i].tel%>
              </p>
            </div>
          </div>
          <% } %>
        </div>
        <div class="kor row"  style="display: none;">
          <%var korA = [
          { name: "히밥", store: "해와달", menu: "한정식", tel: "02-123-2345" },{ name: "웅이", store: "두레한식뷔페", menu: "뷔페1인", tel: "02-123-2345" },{name: "먹갱", store: "황금곱창", menu:"소곱창, 소대창", tel: "02-123-2345"},{name: "햇님", store: "평화연남", menu:"낙곱새", tel: "02-123-2345"},{name: "임슐랭", store: "오문창순대국밥", menu:"순대국밥", tel: "02-123-2345"}];%>
          <% for( var i = 0; i<korA.length; i++ ) { %>
          <div class="card col col-md-6 col-12" style="width: 18rem">
            <img
              src="../static/img/foodCategory/kor<%=i +1%>.webp"
              class="card-img-top"
              alt="..."
            />
            <div class="card-body">
              <p class="card-text">
                <%=korA[i].name%>
              </p>
              <p class="card-text">
                <%=korA[i].store%>
              </p>
              <p class="card-text">
                <%=korA[i].menu%>
              </p>
              <p class="card-text">
                <%=korA[i].tel%>
              </p>
            </div>
          </div>
          <% } %>
        </div>
        <div class="wes row"  style="display: none;">
          <%var wesA = [
          { name: "상해기", store: "마잇다키친", menu: "무한리필코스A", tel: "02-123-2345" },{ name: "문복희", store: "아웃백", menu: "스테이크, 코코넛쉬림프 등", tel: "02-123-2345" }];%>
