          <div class="card col col-md-6 col-12" style="width: 18rem">
            <img
              src="../static/img/foodCategory/des<%=i +1%>.webp"
              class="card-img-top"
              alt="..."
            />
            <div class="card-body">
              <p class="card-text">
                <%=desA[i].name%>
              </p>
              <p class="card-text">
                <%=desA[i].store%>
              </p>
              <p class="card-text">
                <%=desA[i].menu%>
              </p>
              <p class="card-text">
                <%=desA[i].tel%>
              </p>
            </div>
          </div>
          <% } %>
        </div>
      </div>
    </section>
    <%-include("footer.ejs")%>
