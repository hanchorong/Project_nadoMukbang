      var chickenA = [
        { name: "문복희", store: "옛날통닭다리", menu: "대왕닭껍질튀김" },{ name: "HONG SOUND", store: "홈플러스", menu: "당당치킨" },{name: "희밥", store: "교촌, bbq 외 3", menu:"반반콤보, 황금올리브 외 3"},];
